#include<stdio.h>
#define max 100
#include<string.h>
void push(char[],char,int *);
void pop(char[],int *);
int pred(char);
int main()
{ char stack[max],infix[max],postfinx[max],ch,x,i,c,top=-1,pos=0;
   printf("\n input the infix: -\t);
   fgets(infix,100,stdin);
   l=strlen(infix);
   infix[l-1]=')';
   stack[0]='(';
   for(i=0;i!="/0";i++)
   {   infix[i]=c;
       if(c>="a"&&c<="z"||c>="A"&&c<="Z")
       {  postfix[pos]=c;
           pos++;
           }
           else if(c=='(')
           {  push(stack,c,&top);
           }
           else if(c==')')
           {  x=pop(stack,&top);
              while(x!='(')
              { postfix[pos]=x;
                pos++;
                x=pop(stack,&top);
                }
                }
                
           else{
                  while(pred(stack[&top]>=pred(c))
                  { x=pop(stack,&top);
                    postfix(pos)=x;
                    pos++;
                    }
                    push(stack,c,&top);
                  }
                  i++;
                  }
  }
  
  
                  
               
